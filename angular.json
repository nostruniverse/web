{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nostr-world-apps": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "projects/nostr-world-apps/src",
      "prefix": "nw",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/nostr-world-apps",
            "index": "projects/nostr-world-apps/src/index.html",
            "main": "projects/nostr-world-apps/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/nostr-world-apps/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/nostr-world-apps/src/favicon.ico",
              "projects/nostr-world-apps/src/assets"
            ],
            "styles": [
              "projects/nostr-world-apps/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "nostr-world-apps:build:production"
            },
            "development": {
              "browserTarget": "nostr-world-apps:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nostr-world-apps:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/nostr-world-apps/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/nostr-world-apps/src/favicon.ico",
              "projects/nostr-world-apps/src/assets"
            ],
            "styles": [
              "projects/nostr-world-apps/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy"
        }
      }
    },
    "chrome-extension": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/chrome-extension",
      "sourceRoot": "projects/chrome-extension/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path":"./projects/chrome-extension/custom-worker-webpack.ts"
            },
            "outputPath": "dist/chrome-extension",
            "index": "projects/chrome-extension/src/index.html",
            "main": "projects/chrome-extension/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/chrome-extension/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/chrome-extension/src/favicon.ico",
              "projects/chrome-extension/src/assets",
              "projects/chrome-extension/src/manifest.json"
            ],
            "styles": [
              "projects/chrome-extension/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "none"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "chrome-extension:build:production"
            },
            "development": {
              "browserTarget": "chrome-extension:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "chrome-extension:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/chrome-extension/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/chrome-extension/src/favicon.ico",
              "projects/chrome-extension/src/assets"
            ],
            "styles": [
              "projects/chrome-extension/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "nostr-acct-mgmt": {
      "projectType": "library",
      "root": "projects/nostr-acct-mgmt",
      "sourceRoot": "projects/nostr-acct-mgmt/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/nostr-acct-mgmt/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/nostr-acct-mgmt/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/nostr-acct-mgmt/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/nostr-acct-mgmt/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "nostr-chat": {
      "projectType": "library",
      "root": "projects/nostr-chat",
      "sourceRoot": "projects/nostr-chat/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/nostr-chat/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/nostr-chat/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/nostr-chat/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/nostr-chat/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "shared": {
      "projectType": "library",
      "root": "projects/shared",
      "sourceRoot": "projects/shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shared/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/shared/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
